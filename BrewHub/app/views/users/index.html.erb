<body>

      <table>
        <thead>
          <tr>
            <th>Username</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>email</th>
            <th>Show</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.firstname %></td>
            <td><%= user.lastname %></td>
            <td><%= user.email %></td>
            <td><%= link_to 'Show', user %></td>
          </tr>
          <% end %>
        </tbody>
      </table>

<%= button_to 'Register', new_user_path, :method => :get %>

</body>
